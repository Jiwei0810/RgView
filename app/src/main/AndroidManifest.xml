<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rg.rgview">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".transitionManagerAnimation.SharedElementActivity"></activity>
        <activity android:name=".transitionManagerAnimation.DelayedTransitionActivity" />
        <activity android:name=".selfView.SelfViewActivity" />
        <activity android:name=".transitionManagerAnimation.AnimationActivity" />
        <activity android:name=".selfView.bubbleEffect.BubbleActivity" />
        <activity android:name=".selfView.envelopEffect.EnvelopActivity" />
        <activity android:name=".selfView.springActionMenu.SpringActivity" />
        <activity android:name=".selfView.jingYunEffect.JingYunActivity" />
        <activity android:name=".transitionManagerAnimation.SceneTransitionActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>